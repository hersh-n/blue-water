    <!-- Theme JavaScript -->
    <script src="javascripts/creative.js"></script>
        <script src="javascripts/photoswipe.min.js"></script>
    <script src="javascripts/photoswipe-ui-default.min.js"></script>
    <%= javascript_include_tag :all %>


